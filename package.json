{
  "name": "@prabhask5/stellar-engine",
  "version": "1.1.5",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./data": {
      "types": "./dist/data.d.ts",
      "import": "./dist/data.js"
    },
    "./auth": {
      "types": "./dist/entries/auth.d.ts",
      "import": "./dist/entries/auth.js"
    },
    "./stores": {
      "types": "./dist/entries/stores.d.ts",
      "import": "./dist/entries/stores.js"
    },
    "./types": {
      "types": "./dist/entries/types.d.ts",
      "import": "./dist/entries/types.js"
    },
    "./utils": {
      "types": "./dist/entries/utils.d.ts",
      "import": "./dist/entries/utils.js"
    },
    "./actions": {
      "types": "./dist/entries/actions.d.ts",
      "import": "./dist/entries/actions.js"
    },
    "./config": {
      "types": "./dist/entries/config.d.ts",
      "import": "./dist/entries/config.js"
    },
    "./crdt": {
      "types": "./dist/entries/crdt.d.ts",
      "import": "./dist/entries/crdt.js"
    },
    "./email": {
      "types": "./dist/entries/email.d.ts",
      "import": "./dist/entries/email.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "tsc",
    "check": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "dead-code": "knip",
    "dead-code:fix": "knip --fix",
    "cleanup": "npm run lint:fix && npm run format",
    "validate": "npm run check && npm run lint && npm run dead-code",
    "prepare": "git config core.hooksPath .githooks",
    "prepublishOnly": "npm run build",
    "release": "node release.js"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.0",
    "dexie": "^4.2.1",
    "nodemailer": "^6.9.0",
    "y-indexeddb": "^9.0.12",
    "y-protocols": "^1.0.6",
    "yjs": "^13.6.20"
  },
  "peerDependencies": {
    "svelte": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "svelte": {
      "optional": true
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/nodemailer": "^6.4.0",
    "eslint": "^9.39.2",
    "globals": "^17.3.0",
    "knip": "^5.82.1",
    "prettier": "^3.8.1",
    "svelte": "^5.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.54.0"
  }
}
